                        <th>#</td>
                        <th>Program</th>
                        <th>Course</th>
                        <th>Year</th>
                        <th>Class</th>
                        <th>Action1</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data.forEach((i,e)=>{ %>
                        <tr>
                                <td><%=e+1%></td>
                                <td><%=i.programID.name%></td>
                                <td><%=i.course.name%></td>
                                <td><%=i.year%></td>
                                <td><%=i.programID.code + ""+ i.year%></td>
                                <td><a href="/lecturer/grader?program=<%=i.programID.code%>&course=<%=i.course.code%>"><button class="btn btn-primary">Grade class</button></a></td>
                        </tr>
                        <% }) %>
                    </tbody>
                    
                </table>
                  
                
